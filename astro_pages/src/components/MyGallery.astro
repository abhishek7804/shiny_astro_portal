  <script>
    var swiper = new Swiper(".mySwiper", {
      effect: "coverflow", 
      loop: true,
      freeMode: true, 
      centeredSlides: true,
      slidesPerView:1,  
      autoplay: {
        delay: 5000,
        disableOnInteraction: false,
      }, 
      coverflowEffect: {
        rotate: 5,
        stretch:-30,
        depth: -10,
        modifier: 1, 
        slideShadows: true,
      },
      pagination: {
        el: ".swiper-pagination",
        clickable: true
      },
    breakpoints: {
      640: {
        slidesPerView: 1,
        spaceBetween: 20,
      },
      768: {
        slidesPerView: 1,
